<div class="page-data" id="datatable">

    <div style="float: right;">
        <form method="post" action="/transection_search">
            <input type="text" placeholder="Search by Transaction ID.." name="id" >
            <button type="submit" class="a-inside edit">Search</button>
        </form>
    </div>
    <div class="data-table">
        <table border="1" cellpadding="7" cellspacing="7">
<!--            <lable ><b> Transaction List</b></lable>-->
            <tr>
                <th>Transaction ID</th>
                <th>Bank ID</th>
                <th>Check Amount</th>
                <th>Check ID</th>
                <th>From ACC</th>
                <th>Created@</th>

            </tr>

            <% if(data.length){

            for(var i = 0;i < data.length;i++) { %>

            <tr class='clickable-row' data-href='/transview/<%= data[i].id %>'>
                <td><%= data[i].id %></td>
                <td><%= data[i].bank_id %></td>
                <td><%= data[i].cheque_amount %></td>
                <td><%= data[i].cheque_id %></td>
                <td><%= data[i].from_acc %></td>
                <td><%= data[i].timestamp %></td>
             <!--   <td>
                                <a class="a-inside edit" href="/transview/<%=data[i].id%>">View</a>
                </td>-->

            </tr>
            <% }

            }else{ %>
            <tr>
                <td colspan="6">No data available</td>
            </tr>
            <% } %>

        </table>
        <div align="right" style="padding-top: 2%;">
            <% if(data.length){%>
            <a class="a-inside edit" href="/allTransaction_previous/<%=data[0].id%>">Previous</a><a class="a-inside edit" href='/allTransaction_next/<%=data[data.length-1].id%>'>Next</a>
            <%}%>
        </div>
    </div>
</div>

<%- include layouts/footer.ejs %>